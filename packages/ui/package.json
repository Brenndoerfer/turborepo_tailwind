    "@types/eslint": "^8.56.10",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
